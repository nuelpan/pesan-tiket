<%- include partials/head %>
<%- include partials/navAdmin %>

<main>
  <table class="table">
    <thead>
      <tr>
        <td>ID Ticket</td>
        <td>Name</td>
        <td>From</td>
        <td>To</td>
        <td>Date</td>
        <td>Price</td>
        <td>Status</td>
      </tr>
    </thead>
    <tbody>
      <% if(dataTickets) { %>
        <% dataTickets.forEach(ticket => { %>
          <tr>
            <td><%= ticket.id %></td>
            <td><%= ticket.User.first_name %> <%= ticket.User.last_name %></td>
            <td><%= ticket.Route.from %></td>
            <td><%= ticket.Route.to %></td>
            <td><%= convertToDate(ticket.date) %></td>
            <td><%= formatNumber(ticket.Route.price) %></td>
            <td><%= ticket.status %></td>
          </tr>
        <% }) %>
      <% } %>
    </tbody>
  </table>
</main>

<%- include partials/foot %>